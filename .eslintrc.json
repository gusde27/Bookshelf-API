{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "eol-last": "off",
        "no-console": "off",
        //"no-unused-vars": "off",
        "no-tabs": "off",
        "indent": "off",
        "space-before-blocks": "off",
        "no-trailing-spaces": "off",
        "quotes": "off",
        "import/order": "off",
        "no-empty": "off",
        "consistent-return": "off",
        "padded-blocks": "off",
        "no-multiple-empty-lines": "off",
        "spaced-comment": "off",
        "linebreak-style": "off",
        "import/no-unresolved": "off"
    }
}
